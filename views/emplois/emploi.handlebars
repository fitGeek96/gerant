<h4 style="color: rgb(54, 172, 8);">{{title}}</h4>
<div class="container" id="table">
    <div class="row">
        <div class="col s12">
            <table class="responsive-table centered">
                <thead>
                    <tr id="jours">
                        <th>Dimanche</th>
                        <th>Lundi</th>
                        <th>Mardi</th>
                        <th>Mercredi</th>
                        <th>Jeudi</th>
                        <th>Vendredi</th>
                        <th>Samedi</th>

                    </tr>
                </thead>
                <tbody>

                    {{#each jours}}
                    <tr>
                        <td>
                            <p> {{timeD}}</p> <br> {{sportD}}
                        </td>
                        <td>
                            <p> {{timeL}}</p> {{sportL}}
                        </td>
                        <td>
                            <p> {{timeM}}</p> {{sportM}}
                        </td>
                        <td>
                            <p> {{timeMer}}</p> {{sportMer}}
                        </td>
                        <td>
                            <p> {{timeJ}}</p> {{sportJ}}
                        </td>
                        <td>
                            <p> {{timeV}}</p> {{sportV}}
                        </td>
                        <td>
                            <p> {{timeS}}</p> {{sportS}}
                        </td>



                    </tr>

                    {{/each}}



                </tbody>
            </table>



        </div>
    </div>
</div>